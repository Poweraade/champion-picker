{
    "version": 2,
    "builds": [
      {
        "src": "*.py",
        "use": "@vercel/python"
      },
      {
        "src": "src/index.js",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/$1" },
      { "src": "/(.*)", "dest": "/src/index.js" }
    ]
  }
  