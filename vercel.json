{
    "version": 2,
    "builds": [
      {
        "src": "*.py",
        "use": "@vercel/python"
      },
      {
        "src": "src/**",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/$1", "methods": ["GET", "POST", "PUT", "DELETE"] },
      { "src": "/(.*)", "dest": "/src/$1" }
    ]
  }
  